else
  export NODE_PATH="$NODE_PATH:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/.pnpm/esbuild@0.12.17/node_modules/esbuild/bin/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/.pnpm/esbuild@0.12.17/node_modules/esbuild/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/.pnpm/esbuild@0.12.17/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/.pnpm/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/node_modules:/Users/toby/Documents/Movie/Learn/Project/node_modules:/Users/toby/Documents/Movie/Learn/node_modules:/Users/toby/Documents/Movie/node_modules:/Users/toby/Documents/node_modules:/Users/toby/node_modules:/Users/node_modules:/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/esbuild/bin/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/esbuild-http-import/node_modules/esbuild/node_modules"
