#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/prettier@2.3.2/node_modules/prettier/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/prettier@2.3.2/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/node_modules:/Users/toby/Documents/Movie/Learn/Project/node_modules:/Users/toby/Documents/Movie/Learn/node_modules:/Users/toby/Documents/Movie/node_modules:/Users/toby/Documents/node_modules:/Users/toby/node_modules:/Users/node_modules:/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/prettier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/prettier@2.3.2/node_modules/prettier/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/prettier@2.3.2/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/.pnpm/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/node_modules:/Users/toby/Documents/Movie/Learn/Project/node_modules:/Users/toby/Documents/Movie/Learn/node_modules:/Users/toby/Documents/Movie/node_modules:/Users/toby/Documents/node_modules:/Users/toby/node_modules:/Users/node_modules:/node_modules:/Users/toby/Documents/Movie/Learn/Project/jsMPV/packages/setup-koa/node_modules/prettier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
